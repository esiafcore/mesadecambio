@model TransactionReportVM

<!-- ROW 1 -->
<div class="row">
    <div class="col-4 col-xl-2 mb-1">
        Fecha Inicial
    </div>
    <div class="col-5 col-xl-4 mb-1">
        <input asp-for="DateTransaInitial" type="date" id="dateInitial"
               class="form-control border p-1" />
    </div>
    <div class="col-4 col-xl-2 mb-1">
        Fecha Final
    </div>
    <div class="col-5 col-xl-4 mb-1">
        <input asp-for="DateTransaFinal" type="date" id="dateFinal" class="form-control border p-1" />
    </div>
</div>

<!-- BTN PRINT -->
<div class="row">
    <div class="col-3 col-md-4 pt-1 my-1">
        <button type="submit" class="btn btn-sm btn-primary">
            <i class="bi bi-printer-fill"></i> Imprimir
        </button>
    </div>
</div>

