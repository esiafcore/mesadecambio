@model TransactionReportVM

<!-- ROW 1 -->
<div class="row">

    <div class="col-4 col-xl-2">
        Fecha Inicial
    </div>
    <div class="col-5 col-xl-4">
        <input asp-for="DateTransaInitial" type="date" id="dateInitial" class="form-control border p-1" />
    </div>
    <div class="col-4 col-xl-2">
        Fecha Final
    </div>
    <div class="col-5 col-xl-4">
        <input asp-for="DateTransaFinal" type="date" id="dateFinal" class="form-control border p-1" />
    </div>
</div>

